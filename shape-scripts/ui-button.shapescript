shape main{
	h_align="center";
	v_align="center";
	noshadow="true";

	defSize(70,18);
	
	setfillcolor(255,255,255);
	rectangle(0,0,100,100);
	
	if (HasTag("Show Mode", "Static Text")) {
		print("#tag:Label#");
	}
	
	if (HasTag("Show Mode", "UI Field")) {
		print("[#tag:Field#]");
	}
	
}
